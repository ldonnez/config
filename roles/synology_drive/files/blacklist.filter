[Version]
major = 1
minor = 1

[Common]
black_dir_prefix = "/Backup", "/Moments", "/images", "/Camera Uploads", "/PhotoBackup"
max_length = 0
max_path = 0

[File]
black_suffix = ".lnk", ".log", ".pst", ".swp", ".temp", ".tmp", "tags", ".swo"
max_size = 0

[Directory]
black_name = ".cache", ".node-gyp", "bower_components", "node_modules", "vendor", "bin", "deps", "pkg", "dist", "build", ".vim", ".tmux", "tmp", ".venv", "venv", "__pycache__"

[EA]
